{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(css|images|js|fonts)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/coral-data/(.*)",
      "dest": "/coral-data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}